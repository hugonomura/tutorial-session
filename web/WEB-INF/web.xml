<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>EfetuaLogin</servlet-name>
        <servlet-class>controle.EfetuaLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscaCidade</servlet-name>
        <servlet-class>controle.BuscaCidade</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastraUsuario</servlet-name>
        <servlet-class>controle.CadastraUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscaUsuarios</servlet-name>
        <servlet-class>controle.BuscaUsuarios</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EfetuaLogin</servlet-name>
        <url-pattern>/EfetuaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscaCidade</servlet-name>
        <url-pattern>/BuscaCidade</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastraUsuario</servlet-name>
        <url-pattern>/CadastraUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscaUsuarios</servlet-name>
        <url-pattern>/BuscaUsuarios</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/error.jsp</location>
    </error-page>
</web-app>
